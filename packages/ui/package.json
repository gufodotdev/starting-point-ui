{
  "name": "starting-point-ui",
  "version": "0.0.1",
  "description": "A component library built on top of Tailwind CSS",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "import": "./dist/index.js",
      "default": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "eslint": "^9",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8"
  },
  "keywords": [
    "ui",
    "tailwind"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
