{
  "name": "starting-point-ui",
  "version": "0.3.0",
  "description": "A component library built on top of Tailwind CSS. Framework agnostic, easy to customize, open source, and works in any project.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "import": "./dist/index.js",
      "default": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "eslint": "^9",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8"
  },
  "keywords": [
    "tailwindcss",
    "components",
    "shadcn",
    "ui"
  ],
  "license": "MIT",
  "author": "Gufo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gufodotdev/starting-point-ui.git",
    "directory": "packages/ui"
  },
  "homepage": "https://www.startingpointui.com",
  "bugs": {
    "url": "https://github.com/gufodotdev/starting-point-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "tw-animate-css": "^1.4.0"
  }
}
