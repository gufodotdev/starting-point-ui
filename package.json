{
  "name": "starting-point-ui",
  "version": "0.9.1",
  "description": "An open-source component library built for Tailwind CSS.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "import": "./dist/index.js",
      "default": "./dist/index.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src/css/**/*.css",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@floating-ui/dom": "^1.7.4",
    "eslint": "^9",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.7.4",
    "tsup": "^8.3.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8"
  },
  "keywords": [
    "tailwindcss",
    "components",
    "shadcn",
    "ui"
  ],
  "license": "MIT",
  "author": "Gufo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gufodotdev/starting-point-ui.git"
  },
  "homepage": "https://www.startingpointui.com",
  "bugs": {
    "url": "https://github.com/gufodotdev/starting-point-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  }
}
